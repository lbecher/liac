#DATA:
    INT32: x, y, z;
#DATA;

#MAIN:
    SET: x, -1;
    #WNZ: x:
        BLOC: FAZER_SCAN;
    #WNZ;
    SET: x, NOT(x);
    SET: y, MUL(y, -1);
    SET: z, BE(z, y);
    PRINT: "Os numeros que voce digitou: %d %d %d\n", x, y, z;
#MAIN;

#FAZER_SCAN:
    PRINT: "Digite tres nemeros: ";
    SCAN: "%d %d %d", x, y, z;
    #INZ: x:
        #INZ: y:
            PRINT: "Vish";
        #INZ;
    #INZ;
#FAZER_SCAN;